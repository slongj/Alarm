<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alarm.jamesslong.alarm" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".Alarm_Main"
            android:label="@string/app_name"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Add_Alarm"
            android:label="@string/title_activity_add__alarm"
            android:theme="@android:style/Theme.Holo.NoActionBar"
            android:parentActivityName=".Alarm_Main" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.alarm.jamesslong.alarm.Alarm_Main" />
        </activity>
    </application>

</manifest>


